{{#isGET}}
    {{#if isUUID}}
        export function {{&methodName}} (uuid, params) {
            return request({
                url: `{{pathParameter path}}`,
                params
            })
        }
        {{else}}
        export function {{&methodName}} (params) {
            return request({
                url: '{{&path}}',
                params
            })
        }
    {{/if}}
{{/isGET}}

{{#isPOST}}
    {{#if isUUID}}
        export function {{&methodName}} (uuid, data) {
            return request({
                url: `{{pathParameter path}}`,
                method: 'post',
                data
            })
        }
        {{else}}
        export function {{&methodName}} (data) {
            return request({
                url: '{{&path}}',
                method: 'post',
                data
            })
        }
    {{/if}}
{{/isPOST}}